{
  "name": "endpointfinder",
  "displayName": "endpointfinder",
  "description": "Connects endpoints to implementation",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcellourbani/endpointfinder.git"
  },
  "version": "0.0.1",
  "publisher": "murbani",
  "license": "MIT",
  "engines": {
    "vscode": "^1.77.0"
  },
  "keywords": [
    "tsoa",
    "rest"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:/tsoa.json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "endpointfinder.find",
        "title": "Find endpoint"
      },
      {
        "command": "endpointfinder.reload",
        "title": "Refresh endpoint list"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "esbuild": "^0.17.18",
    "eslint": "^8.39.0",
    "jest": "^29.5.0",
    "prettier-eslint": "^15.0.1",
    "ts-jest": "^29.1.0"
  },
  "dependencies": {
    "@tsoa/cli": "^5.1.1",
    "@tsoa/runtime": "^5.0.0"
  }
}